<?xml version="1.0" encoding="utf-8"?>
<com.example.collectedview.Layout.ReboundScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.collectedview.MainActivity"
    >

<android.support.constraint.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     >


   <Button
       android:id="@+id/loading_dialog"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginEnd="8dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="8dp"
       android:textAllCaps="false"
       android:text="LoadingDialog"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toTopOf="parent" />

   <Button
       android:id="@+id/lableView_btn"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginEnd="8dp"
       android:layout_marginStart="8dp"
       android:textAllCaps="false"
       android:layout_marginTop="8dp"
       android:text="lableView"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintEnd_toStartOf="@+id/loading_dialog"
       app:layout_constraintHorizontal_bias="0.5"
       app:layout_constraintStart_toEndOf="@+id/loading_dialog"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toTopOf="parent" />

   <Button
       android:id="@+id/ShepherdCheck"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginStart="8dp"
       android:layout_marginTop="8dp"
       android:text="ShepherdCheckActivity"
       android:textAllCaps="false"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toBottomOf="@+id/lableView_btn" />

   <Button
       android:id="@+id/btn_circle"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginEnd="8dp"
       android:layout_marginStart="8dp"
       android:textAllCaps="false"
       android:text="CircleProgress"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toEndOf="@+id/loading_dialog"
       tools:layout_editor_absoluteY="8dp" />

   <Button
       android:id="@+id/btn_sipnner"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginStart="8dp"
       android:layout_marginTop="8dp"
       android:text="下拉列表"
       app:layout_constraintStart_toEndOf="@+id/ShepherdCheck"
       app:layout_constraintTop_toBottomOf="@+id/loading_dialog" />

   <Button
       android:id="@+id/dialogFragment"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginStart="8dp"
       android:layout_marginTop="8dp"
       android:layout_marginEnd="8dp"
       android:text="Button"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toEndOf="@+id/btn_sipnner"
       app:layout_constraintTop_toBottomOf="@+id/btn_circle" />

</android.support.constraint.ConstraintLayout>
</com.example.collectedview.Layout.ReboundScrollView>